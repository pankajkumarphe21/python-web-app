{
    "version": 2,
    "builds": [
      {
        "src": "schoolproject/asgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "schoolproject/asgi.py" }
    ]
  }
  